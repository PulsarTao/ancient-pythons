
all=	chdir.c.o ¶
	config.c.o ¶
#	dynamic_load.c.o ¶
	gestaltmodule.c.o ¶
	getbootvol.c.o ¶
	getwd.c.o ¶
	macgetmtime.c.o ¶
	macguesstabsize.c.o ¶
